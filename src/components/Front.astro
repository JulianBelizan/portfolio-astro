---
import HtmlIcon from "./icons/Html.astro"
import AstroIcon from "./icons/AstroIcon.astro"
import CssIcon from "./icons/Css.astro"
import JsIcon from "./icons/Javascript.astro"
import ReactIcon from "./icons/React.astro"
import BootstrapIcon from "./icons/Bootstrap.astro"
import SassIcon from "./icons/Sass.astro"
import TailwindIcon from "./icons/Tailwind.astro"
---

<div class="w-52 border-2 border-indigo-400 gap-2 h-48 ">
    <h2 class="font-bold text-xl font-20 text-indigo-600">Front-End</h2>    
    <ul class="flex flex-row gap-x-3 pt-3 justify-center ">
        <HtmlIcon class="size-9" alt="HTML"/>
        <CssIcon class="size-9" alt="Css"/>
        <JsIcon class="size-9" alt="Javascript"/>
    </ul>
    <ul class="flex flex-row gap-x-3 pt-4 justify-center">
        <AstroIcon class="size-9" alt="Astro"/>
        <ReactIcon class="size-9" alt="React"/>
        <BootstrapIcon class="size-9" alt="Bootstrap"/>
    </ul>
    <ul class="flex flex-row gap-x-3 pt-4 justify-center">
        <SassIcon class="size-9" alt="Sass"/>
        <TailwindIcon class="size-9" alt="Tailwind"/>
    </ul>
</div>